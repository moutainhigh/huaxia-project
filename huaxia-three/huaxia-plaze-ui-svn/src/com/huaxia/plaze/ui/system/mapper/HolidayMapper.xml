<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.huaxia.plaze.ui.system.mapper.HolidayMapper">

	<select id="selectBooleanHoliday" parameterType="java.lang.String" resultType="java.lang.Integer">
		SELECT COUNT(1) 
		FROM SYS_HOLIDAY 
		WHERE 
		HOLIDAY_DATE = #{date}
	</select>
	
</mapper>