	<!-- 查询MulBase表-->
	<select id="queryMulBorBase" parameterType="java.lang.String"
	resultMap="result_MulBorBase">
		SELECT 
			UUID  ,
		  CRT_TIME ,
		  CRT_USER  ,
		  PK_UUID ,
		  APP_ID  ,
		  CERT_NO ,
		  TRN_ID,
		  MOBILE,
		  
		  SUCCESS  ,
		  ID ,
		  REASON_CODE ,
		  REASON_DESC ,
		  NEXTSERVICE  ,
		  ERROR_INFO  ,
		  
		  FINAL_SCORE ,
		  FINAL_DECISION,
		  COURT_CLOSE_RISK_1  ,
		  OVERDUE_RISK_2 ,
		  COURT_RISK_3  ,
		  OVERDUE_REPAY_RISK_4  ,
		  COMPANY_TAXOWING_RISK_5 ,
		  COURT_ZHIXING_RISK_6  ,
		  CAR_LOAN_RISK_7    ,
		  TAX_OWING_RISK_8  ,
		  CRIME_RISK_9  ,
		  STUDENT_LOAN_RISK_10 ,
		  VEHICLE_LEASE_DEFAULT_RISK_11,
		  COMPANY_PAYING_RISK_12 ,
		  DELIBERATE_VIOLATION_RISK_13 ,
		  FALSE_MOBILE_RISK_14 ,
		  ALIMOBILE_RISK_15 ,
		  OVERDUE_RISK_16  ,
		  VEHICLE_LEASE_DEFAULT_RISK_17,
		  COMPANY_PAYING_RISK_18  ,
		  OVERDUE_REPAY_RISK_19  ,
		  FOLLOW_HIGH_20   ,
		  FOLLOW_MEDIUM_21 ,
		  FOLLOW_LOW_22    ,
		  FOLLOW_HIGH_23 ,
		  FOLLOW_MEDIUM_24 ,
		  FOLLOW_LOW_25,
		  ext_search_result
		FROM MULBOR_BASE 
			WHERE TRN_ID = #{trnId} and  rownum &lt;2
   </select>
	<!-- 查询MulBorRiskItem表-->
	<select id="queryMulBorRiskItem" parameterType="java.lang.String"
	resultMap="result_MulBorRiskItem">
		SELECT 
		    	UUID ,
			  CRT_TIME,
			  CRT_USER,
			  REF_UUID ,
			  PK_UUID ,
			 APP_ID  ,
		     TRN_ID,
		      SCORE  ,
			  DECISION   ,
			  RISK_NAME  ,
			  POLICY_DECISION ,
			  POLICY_MODE   ,
			  POLICY_SCORE ,
			  POLICY_NAME ,
			  RULE_ID
		FROM MULBOR_RISK_ITEM 
		where	REF_UUID IN (
				select PK_UUID from MULBOR_BASE where TRN_ID = #{trnId} and  rownum&lt;2
				)
   </select>
   	<!-- 查询MulBorBlackList表-->
	<select id="queryMulBorBlackList" parameterType="java.lang.String"
	resultMap="result_MulBorBlackList">
		SELECT 
		    	UUID,
			  CRT_TIME,
			  CRT_USER,
			  REF_UUID,
			  APP_ID  ,
		      TRN_ID,
		      TYPE ,
			  DESCRIPTION,
			  HIT_TYPE_DISPLAY_NAME,
			  FRAUD_TYPE_DISPLAY_NAME,
		  
			  CASE_CODE   ,
			  EXECUTE_COURT   ,
			  CASE_DATE  ,
			  GENDER    ,
			  CARRY_OUT   ,
			  EXECUTED_NAME  ,
			  FRAUD_TYPE  ,
			  SPECIFIC_CIRCUMSTANCES ,
			  EXECUTE_SUBJECT  ,
			  EXECUTE_STATUS ,
			  EVIDENCE_COURT ,
			  TERM_DUTY ,
			  PROVINCE  ,
			  EXECUTE_CODE ,
			  VALUE    ,
			  AGE       ,
			  EVIDENCE_TIME
		FROM MULBOR_BLACK_LIST 
		where	REF_UUID IN (
				  select PK_UUID from MULBOR_RISK_ITEM WHERE REF_UUID IN (
				  select PK_UUID from MULBOR_BASE where TRN_ID = #{trnId} and  rownum&lt;2
				  )
				)
   </select>
   	<!-- 查询MulBorDescreditCount表-->
