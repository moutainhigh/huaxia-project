	<!-- 查询风险名单表-->
	<select id="queryMulBorRiskList" parameterType="java.lang.String"
	resultMap="result_MulBorRiskList">
		select  distinct rule_id,MULBOR_RISK_ITEM.risk_name,score,description,fraud_type_display_name,HIT_TYPE_DISPLAY_NAME,value FROM MULBOR_RISK_ITEM,MULBOR_BLACK_LIST
		WHERE MULBOR_RISK_ITEM.REF_UUID IN (
		select PK_UUID from MULBOR_BASE where  trn_id =  #{trnId} and  rownum &lt;2
		)
		  AND POLICY_NAME = '风险名单' and  MULBOR_BLACK_LIST.ref_uuid = MULBOR_RISK_ITEM.PK_UUID
   </select>
	
	<!-- 查询表-->
	<select id="queryMulBorDiscreditCountMain" parameterType="java.lang.String"
	resultMap="result_MulBorDiscreditCountMain">
	
		select distinct MULBOR_RISK_ITEM.pk_uuid ,risk_name,description,DISCREDIT_TIMES,score FROM MULBOR_RISK_ITEM,MULBOR_DESCREDIT_COUNT
		WHERE MULBOR_RISK_ITEM.REF_UUID IN (
		select PK_UUID from MULBOR_BASE where   trn_id =  #{trnId} and  rownum &lt;2
		)
		 AND POLICY_NAME = '风险名单' and  MULBOR_DESCREDIT_COUNT.ref_uuid = MULBOR_RISK_ITEM.PK_UUID

   </select>
	
	<!-- 查询关注名单-->
	<select id="queryMulBorAttentionList" parameterType="java.lang.String"
	resultMap="result_MulBorAttentionList">
	
select  MULBOR_RISK_ITEM.Pk_Uuid,risk_name,HIT_TYPE_DISPLAY_NAME,score,description,LISTAGG(FRAUD_TYPE_DISPLAY_NAME,',') within group (order by FRAUD_TYPE_DISPLAY_NAME) FRAUD_TYPE_DISPLAY_NAME from    MULBOR_RISK_ITEM,MULBOR_GREY_LIST 
			WHERE MULBOR_RISK_ITEM.REF_UUID IN (
			select PK_UUID from MULBOR_BASE where  trn_id =  #{trnId} and  rownum &lt;2
			)
			AND POLICY_NAME = '关注名单' and  MULBOR_GREY_LIST.ref_uuid = MULBOR_RISK_ITEM.PK_UUID
			GROUP BY MULBOR_RISK_ITEM.Pk_Uuid, risk_name ,description,score,HIT_TYPE_DISPLAY_NAME

   </select>
	
