function showimg()
{
	var ret = ocx.IMGCInitial("","11","");
	if(ret!=0)
	{
			alert("Initial Control Error!");
			return ;
	}
	var ret1 = ocx.IMGCGetURLByMulti("aps||||1|aps|aps001|");
	if(ret1!="")
	{
			alert("["+ret1+"]");
			ocx.IMGCDownload(ret1);
	}	
}

function showimg1()
{
	var ret = ocx.IMGCInitial("","11","");
	if(ret!=0)
	{
			alert("Initial Control Error!");
			return ;
	}
	ocx.IMGCDownloadByCode("aps|1102240920G00291||孙小丽|1|aps|aps001|");
}

