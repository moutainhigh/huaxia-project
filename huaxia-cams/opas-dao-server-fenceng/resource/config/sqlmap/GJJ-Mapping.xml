	<!-- 征信信息页面gjj展示 -->
	<select id="queryGjjInfo" parameterType="java.lang.String" resultMap="gjjBriefInfoResultMap">	
		select f.name,f.phone,f.deposit_amount,f.deposit_base,f.start_date,f.deposit_rate,
             f.balance,f.company, s.consis_months,f.record_date
        from jianbing_gjj_brief f left join jianbing_gjj_brief_analysis s 
        on f.app_Id = s.app_id
        where f.app_id = #{appId,jdbcType=VARCHAR} 
	</select>
	
	<!-- 公积金数据 --> 
	<select id="queryGjjBriefInfo" parameterType="java.lang.String" resultMap="gjjBriefInfoResultMap">	
		select  f.name,f.ID,f.card,f.customer_id,f.balance,f.fb_balance,f.once_balance,
			f.deposit_amount,f.fb_deposit_amount,f.record_date,f.status,f.company,
			f.deposit_base,f.init_date,f.start_date,f.person_rate,f.company_rate,
			f.gender,f.email,f.phone,f.marriage,f.address,f.birthday,f.company_id,
			f.person_deposit_amount,f.company_deposit_amount,f.deposit_rate,f.location ,
			s.deposit_base_analysis,s.record_date_analysis,s.platform_refresh_time,
			s.cont_last_times,s.consis_months,s.sum_times,s.exist_months,s.platform_create_time,
			s.deposit_amount_analysis,s.balance_analysis,s.platform_updated_time,s.location_all,
			s.province,s.city,s.deposit_rate_analysis,s.gjj_status_analysis,
			j.company_12m_arr,j.company_24m_arr,j.created_date_arr,j.company_count_24m_analysis,
			j.company_count_6m_analysis,j.company_count_12m_analysis,j.detail_latest_create_time,
			j.detail_latest_record_month,j.detail_latest_record_date,j.detail_init_create_time,
			j.detail_init_record_month,j.detail_init_record_date,j.detail_times
		from JIANBING_GJJ_BRIEF f left join jianbing_gjj_brief_analysis s on f.app_id = s.app_Id
		left join JIANBING_GJJ_DETAIL_ANALYSIS j on j.app_id = f.app_id
		 where f.app_id = #{appId,jdbcType=VARCHAR} 
	</select>
	
	<!-- 公积金明细 --> 
	<select id="queryGjjDetail" parameterType="java.lang.String" resultMap="gjjDetailResultMap">	
		select gjj_type,company,record_date,op_type,     
			 record_month,amount,balance,std_op_type 
		from JIANBING_GJJ_DETAIL where app_id = #{appId,jdbcType=VARCHAR} 
	</select>
