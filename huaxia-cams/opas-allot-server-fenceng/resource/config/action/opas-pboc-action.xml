<x:beans xmlns:x="http://www.springframework.org/schema/beans" 
	xmlns="http://www.huateng.com/schema/ics-core" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
						http://www.huateng.com/schema/ics-core http://www.huateng.com/schema/ics-core.xsd">
	
	
	<!-- 自动分件模式 -->
	<action id="allotAction" class="com.huaxia.opas.allot.action.AllotAction" /> 
	<action id="allotFastAction" class="com.huaxia.opas.allot.action.AllotFastAction" /> 
	<!--手动分件   -->
	<action id="allotHandAction" class="com.huaxia.opas.allot.action.AllotHandAction" />
	<!-- 开关   -->
	<action id="switchAction" class="com.huaxia.opas.allot.action.SwitchAction" />
	<action id="ruleAction" class="com.huaxia.opas.allot.action.RuleAction" />
	<!-- 挂起  解挂  备注 -->
	<action id="allotCommonAction" class="com.huaxia.opas.allot.action.AllotCommonAction" />
	<!-- 转移 回收-->
	<action id="transferAction" class="com.huaxia.opas.allot.action.TransferAction" />
	<!-- 自动分件查询-->
	<action id="autoAction" class="com.huaxia.opas.allot.action.AutoAction" />
	<!-- 待进入人工查询-->
	<action id="waitingEnterAction" class="com.huaxia.opas.allot.action.WaitingEnterAction" />
</x:beans>